{
  "name": "colyseus-shared",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/colyseus-shared/src",
  "projectType": "library",
  "targets": {
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/colyseus-shared/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/libs/colyseus-shared"],
      "options": {
        "jestConfig": "libs/colyseus-shared/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
