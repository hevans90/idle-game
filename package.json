  "license": "MIT",
  "scripts": {
    "build": "nx build",
    "console": "hasura console --project apps/hasura/db",
    "dump-pg-schema": "PGPASSWORD=postgrespassword pg_dump -h localhost -p 5432 -U postgres --schema-only -f apps/go-server/schema.sql",
    "docker-build": "docker compose --env-file .env build --no-cache",
    "docker-down": "docker compose --env-file .env down",
    "docker-up": "docker compose --env-file .env up -d",
    "export-db:techs": "zsh export-techs.sh",
    "generate": "graphql-codegen --config codegen.js -r dotenv/config --watch",
    "generate:docker": "graphql-codegen --config codegen.docker.js --watch",
    "generate:palette": "yarn nx run theme:generate-palette-watch",
    "generate:watch": "yarn generate --watch",
    "hasura": "yarn metadata-apply && yarn migrate-apply && yarn metadata-reload",
    "metadata-apply": "hasura metadata apply --project apps/hasura/db",
    "metadata-reload": "hasura metadata reload --project apps/hasura/db",
    "migrate-apply": "hasura migrate apply --project apps/hasura/db --database-name default",
    "prepare-watch": "nx build idleverse-game-server && yarn --cwd dist/apps/game-server install --no-lockfile",
    "start": "nx serve",
    "status": "hasura migrate status --project apps/hasura/db --database-name default",
    "test": "nx test",
    "web:build:dev:stats": "nx build idleverse-web --skip-nx-cache -- --memoryLimit=8192 --statsJson",
    "web:build:prod:stats": "nx build idleverse-web --prod --skip-nx-cache -- --memoryLimit=8192 --statsJson",
    "webpack-bundle-analyzer": "webpack-bundle-analyzer dist/apps/web/stats.json"
  },
