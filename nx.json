{
  "npmScope": "idleverse",
  "affected": {
    "defaultBase": "origin/main"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/nx-cloud",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"],
        "accessToken": "MWFjMTZiNDUtOGJmYi00OTY3LTg1MDEtNTQ0ODNjYzQ5NTI5fHJlYWQtd3JpdGU=",
        "parallel": 1
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/react"
  },
  "generators": {
    "@nrwl/react": {
      "application": {
        "style": "css",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "css"
      },
      "library": {
        "style": "css",
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "idleverse-web",
  "targetDefaults": {
    "build": {
      "dependsOn": ["^build"]
    }
  }
}
