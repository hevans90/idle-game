{
  "name": "idleverse",
  "homepage": "https://hevans90.github.io/idleverse",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "prepare-watch": "nx build idleverse-game-server && yarn --cwd dist/apps/idleverse/game-server install --no-lockfile",
    "start": "nx serve",
    "build": "nx build",
    "test": "nx test",
    "generate:palette": "yarn nx run theme:generate-palette-watch",
    "generate:watch": "yarn generate --watch",
    "generate": "graphql-codegen --config codegen.js -r dotenv/config && npx nx format:write --files ./libs/galaxy-gql/src/lib/galaxy-api.ts ./libs/food-gql/src/lib/food-api.ts",
    "console:idleverse": "hasura console --project apps/idleverse/hasura/db",
    "status:idleverse": "hasura migrate status --project apps/idleverse/hasura/db --database-name default",
    "migrate-apply:idleverse": "hasura migrate apply --project apps/idleverse/hasura/db --database-name default",
    "metadata-apply:idleverse": "hasura metadata apply --project apps/idleverse/hasura/db",
    "metadata-reload:idleverse": "hasura metadata reload --project apps/idleverse/hasura/db",
    "hasura:idleverse": "yarn metadata-apply:idleverse && yarn migrate-apply:idleverse && yarn metadata-reload:idleverse",
    "docker-up:idleverse": "docker compose --project-directory apps/idleverse/hasura --env-file .env.idleverse up -d",
    "docker-build:idleverse": "docker compose --project-directory apps/idleverse/hasura --env-file .env.idleverse build --no-cache",
    "docker-down:idleverse": "docker compose --project-directory apps/idleverse/hasura --env-file .env.idleverse down",
    "web:build:prod:stats": "nx build idleverse-web --prod --skip-nx-cache -- --memoryLimit=8192 --statsJson",
    "web:build:dev:stats": "nx build idleverse-web --skip-nx-cache -- --memoryLimit=8192 --statsJson",
    "webpack-bundle-analyzer": "webpack-bundle-analyzer dist/apps/idleverse/web/stats.json"
  },
  "private": true,
