    "food-hasura": {
      "root": "apps/food-hasura",
      "sourceRoot": "apps/food-hasura/src",
      "projectType": "application",

      "targets": {
        "build": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              {
                "command": "echo Ready to run migrations."
              }
            ]
          }
        },
        "deploy": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              {
                "command": "hasura metadata apply --project apps/food-hasura/db --endpoint $FOOD_HASURA_URL  --admin-secret $FOOD_HASURA_ADMIN_SECRET"
              },
              {
                "command": "hasura migrate apply --project apps/food-hasura/db --endpoint $FOOD_HASURA_URL  --admin-secret $FOOD_HASURA_ADMIN_SECRET --database-name default"
              },
              {
                "command": "hasura metadata reload --project apps/food-hasura/db --endpoint $FOOD_HASURA_URL  --admin-secret $FOOD_HASURA_ADMIN_SECRET"
              }
            ]
          }
        }
      },

      "tags": []
    },
